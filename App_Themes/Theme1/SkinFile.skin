    <asp:Calendar runat="server" 
        BackColor="#e9edea" 
        BorderColor="#FFFFFF" 
        CellPadding="1" 
        DayNameFormat="Shortest" 
        Font-Names="Arial" 
        Font-Size="8pt" 
        ForeColor="#000000" 
        Width="160px" 
        >
        <SelectedDayStyle 
            BackColor="#666666" 
            Font-Bold="True" 
            ForeColor="White" 
            />
        <TodayDayStyle BackColor="#CCCCCC" ForeColor="Black" />
        <SelectorStyle BackColor="#CCCCCC" />
        <WeekendDayStyle BackColor="#dadada" />
        <OtherMonthDayStyle ForeColor="#888888" />
        <NextPrevStyle VerticalAlign="Bottom" />
        <DayHeaderStyle cssClass="Calendar"
            BackColor="#dadada" 
            Font-Bold="True" 
            Font-Size="7pt" 
            />
